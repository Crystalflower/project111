my_list_1 = [2, 5, 8, 2, 12, 12, 4]
my_list_2 = [4, 2, 7, 12, 3]

count = 0
while count != len(my_list_1):
    if my_list_1[count] in my_list_2:
        my_list_1.remove(my_list_1[count])
    else:
        count += 1


print (my_list_1)

