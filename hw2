newdate = date.replace('.', '')
day = newdate[:2]
mounth = newdate [2:4]
print("{} {} {} года!".format(day_dict[day], mounth_dict[mounth], newdate[4:]))

my_list_11 = [2, 2, 5, 12, 8, 2, 12]
new_list = []

count = 0
while count != len(my_list_11):
    if my_list_11[count] not in new_list:
        new_list.append(my_list_11[count])
    else:
        count += 1
print(new_list)

